{
  "editor.fontSize": 14,
  "workbench.iconTheme": "material-icon-theme",
  "latex.server.autoDownload": true,
  "latex-workshop.latex.tools": [
    {
      "name": "xelatex",
      "command": "xelatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "-cd",
        "%DOC%"
      ],
      "env": {}
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "xelatex",
      "tools": ["xelatex"]
    }
  ],
  "latex-workshop.view.pdf.viewer": "tab",
  "emmet.triggerExpansionOnTab": true,
  "http.proxySupport": "fallback",
  "http.proxy": "http://127.0.0.1:7890",
  "python.dataScience.askForKernelRestart": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[cpp]": {
    "editor.defaultFormatter": "mjohns.clang-format"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorBlinking": "phase",
  "editor.bracketPairColorization.enabled": true,
  "files.associations": {
    "tsconfig.json": "jsonc",
    "*.wxss": "css"
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.profiles.linux": {
    "zsh": {
      "path": "/bin/zsh"
    },
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
      "*": true,
      "yaml": true,
      "plaintext": true,
      "markdown": true,
      "json": false
    }
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "GitHub Dark",
  "dprint.path": "~/.volta/bin/dprint",
  "terminal.integrated.scrollback": 5000,
  "window.zoomLevel": 1,
  "editor.fontFamily": "'JetBrainsMono Nerd Font Mono' ,'Droid Sans Mono', 'monospace', monospace",
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
